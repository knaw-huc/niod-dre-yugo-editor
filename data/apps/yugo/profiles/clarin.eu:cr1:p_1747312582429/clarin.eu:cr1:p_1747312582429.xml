                            <item ConceptLink="http://www.cidoc-crm.org/cidoc-crm/E21_Person">Person</item>
                            <item>Place</item>
                        </enumeration>
                    </Vocabulary>
                </ValueScheme>
            </Element>
            <Component name="Names" ComponentRef="clarin.eu:cr1:c_1747312582453" CardinalityMin="1" CardinalityMax="1">
